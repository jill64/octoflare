{
  "name": "octoflare",
  "version": "0.11.2",
  "description": "A framework for building GitHub Apps with Cloudflare Worker",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/octoflare/issues",
  "homepage": "https://github.com/jill64/octoflare#readme",
  "files": [
    "dist"
  ],
  "keywords": [
    "cloudflare",
    "workers",
    "github",
    "app",
    "webhook",
    "octokit"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./workers": {
      "types": "./dist/re-exports/workers.d.ts",
      "default": "./dist/re-exports/workers.js"
    },
    "./octokit": {
      "types": "./dist/re-exports/octokit.d.ts",
      "default": "./dist/re-exports/octokit.js"
    },
    "./webhook": {
      "types": "./dist/re-exports/webhook.d.ts",
      "default": "./dist/re-exports/webhook.js"
    },
    "./action": {
      "types": "./dist/action/index.d.ts",
      "default": "./dist/action/index.js"
    },
    "./action/core": {
      "types": "./dist/re-exports/actions/core.d.ts",
      "default": "./dist/re-exports/actions/core.js"
    },
    "./action/github": {
      "types": "./dist/re-exports/actions/github.d.ts",
      "default": "./dist/re-exports/actions/github.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/octoflare.git"
  },
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "test": "vitest run --coverage",
    "lint": "npx prettier --check . && npx eslint && tsc",
    "format": "npx prettier --write ."
  },
  "devDependencies": {
    "@types/node": "20.8.6",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vitest/coverage-v8": "^0.34.6",
    "typescript": "5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.0",
    "@cloudflare/workers-types": "4.20231010.0",
    "@octokit/webhooks-types": "7.3.1",
    "octokit": "3.1.1"
  }
}
